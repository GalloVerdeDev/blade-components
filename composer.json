{
    "name": "galloverde/blade-components",
    "description": "Laravel Blade components",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0",
        "blade-ui-kit/blade-heroicons": "^1.3"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "GalloVerde\\BladeComponents\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GalloVerde\\BladeComponents\\BladeComponentsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true
}
